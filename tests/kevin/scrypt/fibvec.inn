def fib(n) {
  if n <= 1 {
    return 1;
  }
  else {
    return fib(n - 1) + fib(n - 2);
  }
}

def fibvec(n) {
  vec = [1, 1];

  while len(vec) > n {
    pop(vec);
  }

  while len(vec) < n {
    push(vec, fib(len(vec)));
  }

  return vec;
}

print fibvec(0);
print fibvec(3);

top10 = fibvec(10);
print top10;
